{
  "name": "erpnext-timer-app",
  "productName": "ErpNext Timer",
  "version": "1.0.0",
  "description": "An app to track work hours on erpnext.",
  "main": "main/index.js",
  "private": true,
  "scripts": {
    "electron-rebuild": "electron-rebuild -f -w desktopIdle",
    "build": "electron-rebuild && nwb build && npm run package",
    "clean": "nwb clean",
    "start": "npm run server",
    "server": "node dev-server.js --run-electron",
    "test": "nwb test",
    "test:watch": "nwb test --server",
    "package:mac": "del-cli release && electron-packager --overwrite --ignore=\"src|tests|release|public\" --prune --platform=darwin --arch=x64 --out=\"release\" .",
    "package:win": "del-cli release && electron-packager --overwrite --ignore=\"src|tests|release|public\" --prune --platform=win32 --arch=x64 --out=\"release\" .",
    "package": "del-cli release && electron-packager --overwrite --ignore=\"src|tests|release|public\" --prune --arch=x64 --out=\"release\" ."
  },
  "dependencies": {
    "@blueprintjs/core": "^3.1.0",
    "@blueprintjs/table": "^3.1.0",
    "desktop-idle": "^1.1.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-rebuild": "^1.8.2",
    "electron-settings": "^3.2.0",
    "electron-window-state": "^4.1.1",
    "emotion": "^9.2.4",
    "esm": "^3.0.55",
    "keytar": "^4.2.1",
    "modern-normalize": "^0.5.0",
    "moment": "^2.22.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-emotion": "^9.2.4",
    "react-measure": "^2.0.2",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.4.0"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "es5",
    "singleQuote": true,
    "semi": false,
    "jsxBracketSameLine": false,
    "bracketSpacing": true
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "del-cli": "^1.1.0",
    "electron": "^2.0.3",
    "electron-packager": "^12.1.0",
    "express": "^4.16.3",
    "minimist": "^1.2.0",
    "nwb": "0.21.x",
    "nwb-less": "^0.7.1"
  },
  "author": "",
  "license": "MIT",
  "repository": ""
}
