{
  "name": "erpnext-timer-app",
  "productName": "ErpNext Timer",
  "version": "1.0.13",
  "description": "An app to track work hours on erpnext.",
  "main": "main/index.js",
  "private": false,
  "scripts": {
    "update-sentry": "node sentry-symbols.js",
    "ship": "nwb clean && nwb build && electron-builder -p always",
    "flow": "flow",
    "flow-watch": "watch \"cls && flow\" ./src --filter=./flow-filter.js --wait=1 --interval=1 --ignoreDotFiles",
    "clean": "nwb clean",
    "start": "npm run server --no-hmre",
    "server": "node dev-server.js --run-electron",
    "test": "nwb test",
    "test:watch": "nwb test --server",
    "build:icons": "node build_icons.js",
    "build": "nwb clean && nwb build && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.digithinkit.timerapp",
    "files": [
      "!.electron-symbols/**",
      "!releases/**",
      "!test/**",
      "!*.log",
      "!*.dist"
    ],
    "directories": {
      "output": "releases",
      "app": "."
    },
    "publish": {
      "provider": "s3",
      "bucket": "timer-app",
      "acl": "public-read"
    },
    "dmg": {},
    "win": {
      "target": "NSIS"
    }
  },
  "dependencies": {
    "@blueprintjs/core": "^3.1.0",
    "@blueprintjs/datetime": "^3.3.0",
    "@blueprintjs/select": "^3.1.0",
    "@blueprintjs/table": "^3.1.0",
    "@sentry/electron": "^0.12.1",
    "bloom-conditionals": "^1.0.1",
    "bloom-day-timeline": "ssh://git@github.com:DigiThinkIT/bloom-day-timeline.git",
    "bloom-nav-column": "ssh://git@github.com:DigiThinkIT/bloom-nav-column.git",
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "desktop-idle": "^1.1.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-log": "^2.2.17",
    "electron-settings": "^3.2.0",
    "electron-updater": "^3.1.2",
    "electron-window-state": "^4.1.1",
    "emotion": "^9.2.4",
    "esm": "^3.0.55",
    "jquery": "^3.3.1",
    "keytar": "^4.2.1",
    "modern-normalize": "^0.5.0",
    "moment": "^2.22.2",
    "moment-duration-format": "^2.2.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.11",
    "react-measure": "^2.0.2"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "es5",
    "singleQuote": true,
    "semi": false,
    "jsxBracketSameLine": false,
    "bracketSpacing": true
  },
  "devDependencies": {
    "@sentry/wizard": "^0.12.1",
    "babel-flow-types": "^1.2.3",
    "babel-preset-env": "^1.7.0",
    "del-cli": "^1.1.0",
    "electron": "^2.0.3",
    "electron-builder": "^20.28.4",
    "electron-packager": "^12.1.0",
    "electron-rebuild": "^1.8.2",
    "express": "^4.16.3",
    "flow-bin": "^0.82.0",
    "minimist": "^1.2.0",
    "nwb": "0.21.x",
    "nwb-less": "^0.7.1",
    "png2icons": "^1.0.1",
    "sharp": "^0.21.0",
    "watch": "^1.0.2"
  },
  "author": "",
  "license": "MIT",
  "repository": ""
}
